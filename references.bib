@article{rukhsarAnalyzingRNASeqGene2022,
  title        = {Analyzing {{RNA-Seq Gene Expression Data Using Deep Learning Approaches}} for {{Cancer Classification}}},
  author       = {Rukhsar, Laiqa and Bangyal, Waqas Haider and Ali Khan, Muhammad Sadiq and Ag Ibrahim, Ag Asri and Nisar, Kashif and Rawat, Danda B.},
  date         = {2022-01},
